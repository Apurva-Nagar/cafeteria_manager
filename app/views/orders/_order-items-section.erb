<section class="OrderItemSection">
  <h3 class="OrderItem-details">Order ID: <%= order_id %> | Customer: <%= customer_name %></h3>
  <hr>
  <% order_items.all.each do |item| %>
    <div class="OrderItem-container">
      <p class="OrderItem-item"><%= item.menu_item_name %></p>
      <p class="OrderItem-item">&#8377; <%= item.menu_item_price %></p>
    </div>
  <% end %>
</section>
